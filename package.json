{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:worker": "npx @opennextjs/cloudflare build",
    "dev:worker": "npx wrangler pages dev .open-next/assets --compatibility-flag=nodejs_compat",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && npx wrangler pages deploy .open-next/assets"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@node-rs/bcrypt": "^1.10.7",
    "@opennextjs/cloudflare": "^0.5.11",
    "next": "14.2.24",
    "random-word-slugs": "^0.1.7",
    "react": "^18",
    "react-dom": "^18",
    "react-share": "^5.2.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250312.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.24",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^4.0.0"
  }
}
